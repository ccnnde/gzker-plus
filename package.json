{
  "name": "gzker-plus",
  "version": "1.0.0",
  "private": true,
  "description": "过早客论坛增强插件～",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccnnde/gzker-plus.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Nor Cod",
    "email": "norfecod@outlook.com"
  },
  "type": "module",
  "scripts": {
    "build": "pnpm clean:dist && vue-tsc && vite build",
    "build:ff": "TARGET=firefox pnpm build",
    "build:watch": "pnpm clean:dist && vite build --watch --mode development",
    "build:watch:ff": "TARGET=firefox pnpm build:watch",
    "clean:dist": "rimraf dist",
    "clean:node": "rimraf node_modules",
    "dev": "pnpm clean:dist && vite",
    "dev:ff": "TARGET=firefox pnpm dev",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "lint:list": "DEBUG=eslint:cli-engine pnpm lint",
    "prepare": "husky install",
    "styl-lint": "stylelint **/*.{scss,vue}",
    "styl-lint:fix": "stylelint **/*.{scss,vue} --fix",
    "styl-lint:list": "pnpm styl-lint -f verbose",
    "up-all": "pnpm up --latest"
  },
  "dependencies": {
    "pinia": "^2.0.36",
    "vue": "^3.3.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@commitlint/cli": "^17.6.3",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "eslint": "^8.40.0",
    "eslint-config-alloy": "^5.0.0",
    "eslint-plugin-vue": "^9.12.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "stylelint": "^15.6.1",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-html-minifier": "^1.0.3",
    "vite-plugin-web-extension": "^3.0.7",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.6.5",
    "webextension-polyfill": "^0.10.0"
  }
}
